<h1>Student Form</h1>
<%= form_tag @student do |s| %>
  <%# <%= s.label :first_name %> %>
  <label>First Name</label>
  <%= text_field_tag :"first_name" %>
  <%# <%= s.label :last_name %> %>
  <label>Last Name</label>
  <%= text_field_tag :"last_name" %>
  <%= submit_tag "Submit Student" %>
<% end %>
